{
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "rules": {
    "indent": [
      1
    , 2
    , {
        "SwitchCase": 1
      }
    ]
  , "quotes": [
      2
    , "single"
    ]
  , "linebreak-style": [
      2
    , "unix"
    ]
  , "semi": [
      2
    , "never"
    ]
  , "no-unused-vars": [
      1
    , {
        "vars": "all"
      , "args": "none"
      }
    ]
  , "no-console": [
      1
    ]
  , "no-undef": [
      1
    ]
  , "no-extra-boolean-cast": [
      1
    ]
  }
, "env": {
  "node": true
, "es6": true
}
, "extends": "eslint:recommended"
}
